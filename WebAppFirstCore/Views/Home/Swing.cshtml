@{
    ViewData["Title"] = "Swing";
}

<h1>Swing</h1>

<form asp-controller="Home" asp-action="Swing" method="post">
    <input name="angle" type="number" min="0.1" max="89.1" placeholder="Angle" />
    <input name="velocity" type="number" min="0.1" max="109.1" placeholder="Velocity" />
    <button>Calculate</button>
</form>

@if (ViewBag.Distance != null)
{
    <h3>Result</h3>
    <p>@ViewBag.Distance</p>
}